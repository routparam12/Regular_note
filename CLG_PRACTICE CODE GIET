#lists
lists= [1,3,5,2]
lists.sort()
#list.reverse()
print(lists)

lists= [1,3,5,2]
print(lists[ :3])

#append
lists= [1,3,5,2]
lists.append(7) #append - add in last
print(lists)

#pop
lists= [1,3,5,2]
lists.pop(1)
print(lists)

#remove
lists= [1,3,5,2]
lists.remove(5)
print(lists)

#Tuple
a=(1,3,5,6,2)
print(a.count(2))
print(a.index(2))

#Dictionaries
a={
    "key":"value",
    "name":"myname",
    "marks":"100",
    "list":[1,3,5,6,3]
}
print(a["marks"])
print(a.items())
print(a.keys())
print(a.values())
#display all the keys

dic={
    "name":"roll",
    "Rishabh":53,
    "paramjeet":50,
    "Balbir":11
    }
print(dic.keys())
#update a value
dic.update({"name":"stname"})
print(dic)
#print specific values
print(dic.get("paramjeet"))
#print tuple and find repeting values
t=(2,4,56,2,33,3,2)
print(t.count(2))

#Sets
a={1,2,4,5}
print(len(a)) #Find length of a set
a.remove(4) #Removes 8 from the set
print(a)
a.clear() #clears every element from the set
print(a)

set1={1,2,4,6}
set2={1,3,6,5}

setdiff=set1.difference(set2) #Displays the difference of the both sets
print(setdiff)
setsame=set1.intersection(set2) #Displays the similar element of both sets
print(setsame)
setunion=set1.union(set2) #Displays all the values in a new set
print(setunion)

